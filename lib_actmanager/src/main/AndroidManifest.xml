<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yh.actmanager">

    <uses-permission android:name="android.permission.REORDER_TASKS" />

    <permission
        android:name="${APPLICATION_ID}.permissions.act_process"
        android:protectionLevel="signature" />

    <uses-permission android:name="${APPLICATION_ID}.permissions.act_process" />

    <application>
        <service
            android:name=".internal.ActStateRecService"
            android:exported="false">
            <intent-filter>
                <action android:name="${APPLICATION_ID}.action.ACT_STATE_MESSENGER" />
            </intent-filter>
        </service>
        <service android:name=".internal.module.MainProcessModule$MainService"
            android:exported="false">
            <intent-filter>
                <action android:name="${APPLICATION_ID}.action.MAIN_PROCESS" />
            </intent-filter>
        </service>
    </application>

</manifest>
